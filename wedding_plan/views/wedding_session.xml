<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record id="wedding_session_view_tree" model="ir.ui.view">
            <field name="name">wedding.session.view.tree</field>
            <field name="model">wedding.session</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name" required="1"/>
                    <field name="time" required="1"/>
                </tree>
            </field>
        </record>
    
        <record id="wedding_session_action" model="ir.actions.act_window">
            <field name="name">Wedding Session</field>
            <field name="res_model">wedding.session</field>
            <field name="view_mode">tree</field>
        </record>
    
    </data>
    

</odoo>
